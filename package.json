    "@types/express": "^4.17.9",
    "@types/mocha": "^8.2.0",
    "@types/pino": "^6.3.4",
    "@typescript-eslint/eslint-plugin": "^4.10.0",
    "@typescript-eslint/parser": "^4.10.0",
    "chai": "^4.2.0",
    "eslint": "^7.16.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-prettier": "^3.3.0",
    "mocha": "^8.2.1",
    "nodemon": "^2.0.6",
    "pino-pretty": "^4.3.0",
    "prettier": "^2.2.1",
    "shelljs": "^0.8.4",
    "supertest": "^6.0.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
