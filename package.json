{
  "name": "fil-cosigner",
  "version": "0.1.0",
  "author": "Fission",
  "contributors": [
    "Brooklyn Zelenka <brooklyn@fission.codes>",
    "James Walker <james@fission.codes>",
    "Daniel Holmgren <daniel@fission.codes>",
    "Benjamin Bollen <ben@mosaiclabs.eu>"
  ],
  "description": "Fission FileCoin Cosigner",
  "main": "index.js",
  "scripts": {
    "start": "node dist/index.js",
    "compile": "ts-node build.ts && tsc",
    "dev": "nodemon server/index.ts | pino-pretty",
    "dev:debug": "nodemon --exec \"node -r ts-node/register --inspect-brk\" server/index.ts | pino-pretty",
    "lint": "eslint -c .eslintrc.js \"{server, test}/**/*.{js,ts,tsx}\" --quiet",
    "lint:fix": "eslint -c .eslintrc.js \"{server, test}/**/*.{js,ts,tsx}\" --quiet --fix",
    "test": "mocha -r ts-node/register src/**/*.test.ts --exit",
    "test:debug": "mocha -r ts-node/register --inspect-brk src/**/*.test.ts --exit"
  },
  "dependencies": {
    "@glif/filecoin-address": "^1.1.0-beta.18",
    "@glif/filecoin-message": "^1.1.0-beta.17",
    "base32-decode": "^1.0.0",
    "base32-encode": "^1.1.1",
    "blakejs": "^1.1.0",
    "bn.js": "^5.1.3",
    "body-parser": "^1.19.0",
    "cids": "^1.1.5",
    "cookie-parser": "^1.4.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "fp-ts": "^2.9.3",
    "ipld-dag-cbor": "^0.17.0",
    "leb128": "^0.0.5",
    "lowercase-keys": "^2.0.0",
    "noble-bls12-381": "^0.8.0",
    "pino": "^6.8.0",
    "swagger-express-middleware": "^4.0.2"
  },
  "devDependencies": {
    "@types/chai": "^4.2.14",
    "@types/express": "^4.17.9",
    "@types/mocha": "^8.2.0",
    "@types/pino": "^6.3.4",
    "@typescript-eslint/eslint-plugin": "^4.10.0",
    "@typescript-eslint/parser": "^4.10.0",
    "chai": "^4.2.0",
    "eslint": "^7.16.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-prettier": "^3.3.0",
    "mocha": "^8.2.1",
    "nodemon": "^2.0.6",
    "pino-pretty": "^4.3.0",
    "prettier": "^2.2.1",
    "shelljs": "^0.8.4",
    "supertest": "^6.0.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  }
}
